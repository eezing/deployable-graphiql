{
  "scripts": {
    "start": "IS_PROXY=true node server",
    "dev": "NODE_ENV=development PORT=3000 node server.js",
    "pretty": "prettier --write '**/*.js'",
    "test": "rm -rf out || true && npm run test:jest && npm run test:lint",
    "test:watch": "jest --watch",
    "test:jest": "jest --coverage",
    "test:lint": "eslint ./ --ext .js --ext .mjs",
    "test:gitunstaged": "if ! [[ -z $(git diff) ]]; then echo \"tree has un-staged changes\"; exit 1; fi",
    "precommit": "npm run test:gitunstaged && npm test",
    "build": "next build",
    "build:static": "npm run build && next export",
    "now:demo": "now --local-config=now-demo.json && now ln --local-config=now-demo.json"
  },
  "name": "graphiql-and-more",
  "main": "index.js",
  "author": "Eric Zingeler",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "body-parser": "^1.18.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "prettier": "^1.13.7",
    "react-test-renderer": "^16.4.1"
  },
  "dependencies": {
    "@zeit/next-css": "^0.2.0",
    "graphiql": "^0.11.11",
    "graphql": "^0.13.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonlint-mod": "^1.7.2",
    "next": "^6.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-modal": "^3.5.1",
    "url-search-params": "^1.0.2"
  }
}
